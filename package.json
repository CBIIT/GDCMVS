    "ejs": "^3.1.3",
    "elasticsearch": "^15.2.0",
    "express": "^4.17.1",
    "googleapis": "^54.1.0",
    "helmet": "^3.23.3",
    "lodash": "^4.17.19",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
