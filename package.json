{
  "name": "gdcmvs",
  "version": "v2.2.13",
  "description": "GDC Metadata Validation Services",
  "main": "app.js",
  "scripts": {
    "convert": "node tests/convert.js && node tests/fileList.js",
    "run-some-test": "node tests/skipTest.js && node tests/runSomeTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "test": "node tests/convert.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "skip-test": "node tests/skipTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "build": "webpack --config webpack/webpack.prod.config.js --mode=production",
    "watch": "webpack --watch --config webpack/webpack.dev.config.js --mode=development --devtool=eval-source-map",
    "gdcdocs": "webpack --config webpack/webpack.docs.config.js --mode=production",
    "start": "node app.js"
  },
  "author": "CBIIT",
  "license": "ISC",
