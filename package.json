{
  "name": "gdcmvs",
  "version": "v2.0.1",
  "description": "GDC Metadata Validation Services",
  "main": "app.js",
  "scripts": {
    "convert": "node tests/convert.js && node tests/fileList.js",
    "run-some-test": "node tests/skipTest.js && node tests/runSomeTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "test": "node tests/convert.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "skip-test": "node tests/skipTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "build": "webpack --config webpack/webpack.prod.config.js --mode=production",
    "watch": "webpack --watch --config webpack/webpack.dev.config.js --mode=development --devtool=eval-source-map",
    "gdcdocs": "webpack --config webpack/webpack.docs.config.js --mode=production",
    "start": "node app.js"
  },
  "author": "CBIIT",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/CBIIT/GDCMVS"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "commander": "^2.20.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.7.1",
    "elasticsearch": "^15.2.0",
    "express": "^4.17.1",
    "googleapis": "^27.0.0",
    "helmet": "^3.21.1",
    "lodash": "^4.17.15",
    "method-override": "^2.3.10",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "rotating-file-stream": "^1.4.4",
    "swagger-ui-express": "^4.1.2",
    "winston": "^2.4.4",
    "winston-daily-rotate-file": "^1.7.2",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^5.2.0",
    "node-excel-export": "^1.4.4",
    "node-xlsx": "^0.12.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "selenium-webdriver": "^3.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
