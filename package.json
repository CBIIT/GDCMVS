{
  "name": "gdcmvs",
  "version": "v2.2.19",
  "description": "GDC Metadata Validation Services",
  "main": "app.js",
  "scripts": {
    "convert": "node tests/convert.js && node tests/fileList.js",
    "run-some-test": "node tests/skipTest.js && node tests/runSomeTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "test": "node tests/convert.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "skip-test": "node tests/skipTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "build-webpack": "webpack --config webpack/webpack.prod.config.js --mode=production",
    "build": "rspack --config webpack/rspack.dev.config.js --mode=production",
    "watch": "rspack --watch --config webpack/rspack.dev.config.js --mode=development --devtool=eval-source-map",
    "gdcdocs": "webpack --config webpack/webpack.docs.config.js --mode=production",
    "start": "node app.js"
  },
  "author": "CBIIT",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/CBIIT/GDCMVS"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "commander": "^5.1.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "elasticsearch": "^15.2.0",
    "express": "^4.17.1",
    "googleapis": "^54.1.0",
    "helmet": "^3.23.3",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "rotating-file-stream": "^2.1.3",
    "swagger-ui-express": "^4.1.4",
    "winston": "^2.4.4",
    "winston-daily-rotate-file": "^1.7.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@rspack/cli": "^1.2.2",
    "@rspack/core": "^1.2.2",
    "@rspack/dev-server": "^1.0.10",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "mocha": "^8.0.1",
    "node-excel-export": "^1.4.4",
    "node-xlsx": "^0.15.0",
    "selenium-webdriver": "^3.5.0"
  }
}
