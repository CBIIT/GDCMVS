{
  "name": "gdcmvs",
  "version": "v2.2.28",
  "description": "GDC Metadata Validation Services",
  "main": "app.js",
  "workspaces": [
    "client"
  ],
  "scripts": {
    "convert": "node tests/convert.js && node tests/fileList.js",
    "run-some-test": "node tests/skipTest.js && node tests/runSomeTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "test": "node tests/convert.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "skip-test": "node tests/skipTest.js && mocha tests/output_js_tests/ --reporter xunit --reporter-options output=tests/output_js_tests/e2e_results.xml",
    "watch": "npm run watch --workspace=client",
    "build": "npm run build --workspace=client",
    "start": "node app.js"
  },
  "author": "CBIIT",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/CBIIT/GDCMVS"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "elasticsearch": "^16.7.3",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "node-xlsx": "^0.24.0",
    "rotating-file-stream": "^2.1.6",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.28.0",
    "exceljs": "^4.4.0",
    "mocha": "^11.5.0",
    "selenium-webdriver": "^4.33.0"
  }
}
